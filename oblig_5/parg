










function toggle_par(indata1: std_logic_vector) return std_logic is 
	signal toggle : std_logic := '0' 
	for i in indata1'range loop 
		of indata1(i) = '1' then 
			toggle := not toggle; 
		end if; 
	end loop; 
	return toggle; 
end toggle_par; 

function xor_parity(indata2: unsigned) return std_logic is 
	signal xor_par : std_logic := '0';

	xor_par <= xor(indata2)
	return xor_par; 
end xor_parity;
	
